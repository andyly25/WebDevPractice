<!-- This here is our results page. The page we go to when we click the enter/submit button -->

<h1>Results page</h1>

<ul>
    <!-- Here we loop through the Search variable results for each movie foound -->
    <% data["Search"].forEach(function(movie){ %>
        <!-- separate by li and then just use attributes to get what you need -->
        <li> 
            <strong> 
                <%= movie["Title"] %> 
            </strong> 
            - <%= movie["Year"] %> 
        </li>
    <% }) %>
</ul>

<!-- a simple redirect link to let you to search again -->
<a href="/" title="">Search Again</a>